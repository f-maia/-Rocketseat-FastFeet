{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:debug": "nodemon --inspect src/server.ts",
    "queue": "nodemon src/queue.ts",
    "queue:bebug": "nodemon --inspect src/queue.ts",
    "dbconfig": "sequelize db:create fastfeet && sequelize db:migrate",
    "postdbconfig": "sequelize db:seed admin-user",
    "preseeds:pg": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "seeds:pg": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "seeds:mongo": "seed -u 'mongodb://localhost:27017/fastfeet' --drop-database src/database/mongo/seeds",
    "seeds": "cross-var $npm_execpath run seeds:mongo && cross-var $npm_execpath run seeds:pg",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports",
    "lint": "eslint './src/**/*.{js,ts}' --quiet --fix",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.3",
    "cors": "^2.8.5",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.5",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "youch": "^2.0.10",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/express-handlebars": "^3.1.0",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.6",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.9.0",
    "@types/nodemailer": "^6.4.0",
    "@types/yup": "^0.26.32",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "cross-var": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "mongo-seeding-cli": "^3.4.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.12.1",
    "typescript": "^3.8.3"
  }
}